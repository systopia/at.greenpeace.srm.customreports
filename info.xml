<?xml version="1.0"?>
<extension key="at.greenpeace.srm.customreports" type="module">
  <file>customreports</file>
  <name>Greenpeace SRM Custom reports</name>
  <description>Provides customized data export for Greenpeace SRM.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/systopia/at.greenpeace.srm.customreports</url>
    <url desc="Documentation">http://github.com/systopia/at.greenpeace.srm.customreports</url>
    <url desc="Support">http://github.com/systopia/at.greenpeace.srm.customreports/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-06-22</releaseDate>
  <version>0.4</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Customreports</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
